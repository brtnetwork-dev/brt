{
  "name": "brt-dashboard",
  "version": "1.0.0",
  "description": "BRT web dashboard",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
